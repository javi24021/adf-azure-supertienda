					"transformations": [
						{
							"name": "cast1"
						},
						{
							"name": "cast2"
						}
					],
					"scriptLines": [
						"source(output(",
						"          OrderID as string,",
						"          ProductID as string,",
						"          CustomerID as string,",
						"          SalesPersonID as string,",
						"          OrderDate as string,",
						"          ShipDate as string,",
						"          OrderStatus as string,",
						"          ShipAddress as string,",
						"          BillAddress as string,",
						"          Quantity as string,",
						"          Sales as string,",
						"          CreationTime as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> source1",
						"source1 cast(output(",
						"          OrderID as integer,",
						"          ProductID as integer,",
						"          CustomerID as integer,",
						"          SalesPersonID as integer,",
						"          OrderDate as date 'yyyy-MM-dd',",
						"          ShipDate as date 'yyyy-MM-dd',",
						"          Quantity as integer,",
						"          Sales as integer",
						"     ),",
						"     errors: true) ~> cast1",
						"cast1 cast(output(",
						"          CreationTime as date 'yyyy-MM-dd'",
						"     ),",
						"     errors: true) ~> cast2",
						"cast2 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          OrderID as integer,",
						"          ProductID as integer,",
						"          CustomerID as integer,",
						"          SalesPersonID as integer,",
						"          OrderDate as date,",
						"          ShipDate as date,",
						"          OrderStatus as string,",
						"          ShipAddress as string,",
						"          BillAddress as string,",
						"          Quantity as integer,",
						"          Sales as integer,",
						"          CreationTime as binary",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          OrderID,",
						"          ProductID,",
						"          CustomerID,",
						"          SalesPersonID,",
						"          OrderDate,",
						"          ShipDate,",
						"          OrderStatus,",
						"          ShipAddress,",
						"          BillAddress,",
						"          Quantity,",
						"          Sales,",
						"          CreationTime",
						"     )) ~> sink1"
					]
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/df_silver_employees')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"folder": {
					"name": "SILVER"
				},
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "ds_sql_bronze_employees",
								"type": "DatasetReference"
							},
							"name": "source1"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "ds_sql_silver_employees",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "cast1"
						},
						{
							"name": "cast2"
						}
					],
					"scriptLines": [
						"source(output(",
						"          EmployeeID as string,",
						"          FirstName as string,",
						"          LastName as string,",
						"          Department as string,",
						"          Gender as string,",
						"          Salary as string,",
						"          ManagerID as string,",
						"          BirthDate as date",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     query: '\\nselect EmployeeID,FirstName, ISNULL(LastName,\\'n/a\\')as LastName,Department,Gender,Salary,ISNULL(ManagerID,1) as ManagerID, BirthDate from [dbo].[Employees]',",
						"     format: 'query') ~> source1",
						"source1 cast(output(",
						"          EmployeeID as integer,",
						"          Salary as integer,",
						"          ManagerID as integer",
						"     ),",
						"     errors: true) ~> cast1",
						"cast1 cast(output(",
						"          BirthDate as date 'yyyy-MM-dd'",
						"     ),",
						"     errors: true) ~> cast2",
						"cast2 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          EmployeeID as integer,",
						"          FirstName as string,",
						"          LastName as string,",
						"          Department as string,",
						"          BirthDay as date,",
						"          Gender as string,",
						"          Salary as integer,",
						"          ManagerID as integer",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          EmployeeID,",
						"          FirstName,",
						"          LastName,",
						"          Department,",
						"          BirthDay = BirthDate,",
						"          Gender,",
						"          Salary,",
						"          ManagerID",
						"     )) ~> sink1"
					]
				}
			},
			"dependsOn": []
		},
